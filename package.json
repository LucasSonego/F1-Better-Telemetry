    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.1"
  },
  "main": "main.js",
  "scripts": {
    "react-start": "rescripts start",
    "react-build": "rescripts build",
    "react-test": "rescripts test --env=jsdom",
