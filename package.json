  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "cross-env": "^7.0.3",
    "electron": "^11.2.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "f1-telemetry-client": "^0.1.18",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
